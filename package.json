{
  "name": "strapi-plugin-local-image-sharp",
  "version": "2.0.0",
  "description": "Dynamically resize, format and optimize images based on url modifiers",
  "strapi": {
    "displayName": "Local Image Sharp",
    "name": "local-image-sharp",
    "description": "Dynamically resize, format and optimize images based on url modifiers",
    "required": false,
    "kind": "plugin"
  },
  "keywords": [
    "strapi",
    "plugin",
    "images",
    "sharp",
    "optimize",
    "resize"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "format": "prettier --write \"server/src/**/*.{ts,js}\"",
    "release": "npx standard-version && git push --follow-tags origin main && npm publish",
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "etag": "^1.8.1",
    "ipx": "^3.0.1",
    "ohash": "^1.1.4",
    "qs": "^6.13.0",
    "ufo": "^1.5.4",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@giscus/vue": "^3.0.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@koa/router": "^12.0.1",
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.1.1",
    "@strapi/typescript-utils": "^5.1.1",
    "@types/eslint__js": "^8.42.3",
    "@types/etag": "^1.8.3",
    "@types/koa": "^2.15.0",
    "@types/koa__router": "^12",
    "@types/node": "^20",
    "@types/qs": "^6.9.16",
    "eslint": "^9.13.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vitepress": "^1.4.1"
  },
  "author": {
    "name": "Sacha Stafyniak",
    "email": "sacha@digisquad.io",
    "url": "https://digisquad.io"
  },
  "maintainers": [
    {
      "name": "Strapi Community",
      "url": "https://github.com/strapi-community"
    },
    {
      "name": "Sacha Stafyniak",
      "email": "sacha@digisquad.io",
      "url": "https://digisquad.io",
      "lead": true
    }
  ],
  "bugs": {
    "url": "https://github.com/strapi-community/strapi-plugin-local-image-sharp/issues"
  },
  "homepage": "https://github.com/strapi-community/strapi-plugin-local-image-sharp",
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
}
